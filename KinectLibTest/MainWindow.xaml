        <Image x:Name="leftHand" Source="images/leftHand.png" Canvas.Left="0" Canvas.Top="0" Panel.ZIndex="1" />
        <Image x:Name="rightHand" Source="images/rightHand.png" Canvas.Left="0" Canvas.Top="0" Panel.ZIndex="1" />
        <Grid Canvas.Left="0" Canvas.Top="61" Height="488" Name="grid1" Width="778">
