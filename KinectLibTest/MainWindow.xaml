                <RowDefinition Height="29*" />
                <RowDefinition Height="26*" />
                <RowDefinition Height="26*" />
                <RowDefinition Height="31*" />
                <RowDefinition Height="27*" />
            </Grid.RowDefinitions>
            <TextBlock Height="27" HorizontalAlignment="Left" Margin="0,6,0,0" Name="textBlock1" Text="TextBlock" VerticalAlignment="Top" Width="739" Foreground="#FFEB0F0F" />
            <TextBlock Height="27" HorizontalAlignment="Left" Name="textBlock2" Text="TextBlock" VerticalAlignment="Top" Width="739" Grid.Row="1" Foreground="#FFEB0F0F" />
            <TextBlock Height="27" HorizontalAlignment="Left" Name="textBlock3" Text="TextBlock" VerticalAlignment="Top" Width="739" Grid.Row="2" Foreground="#FFEB0F0F" />
            <TextBlock Height="27" HorizontalAlignment="Left" Name="textBlock4" Text="TextBlock" VerticalAlignment="Top" Width="739" Grid.Row="3" Foreground="#FFEB0F0F" />
            <TextBlock Height="27" HorizontalAlignment="Left" Margin="1,2,0,0" Name="textBlock5" Text="TextBlock" VerticalAlignment="Top" Width="739" Grid.Row="4" Foreground="#FFEB0F0F" />
            <TextBlock Height="27" HorizontalAlignment="Left" Margin="-3,2,0,0" Name="textBlock6" Text="TextBlock" VerticalAlignment="Top" Width="739" Grid.Row="5" Grid.RowSpan="2" Foreground="#FFEB0F0F" />
            <TextBlock Height="27" HorizontalAlignment="Left" Margin="0,1,0,0" Name="textBlock7" Text="TextBlock" VerticalAlignment="Top" Width="739" Grid.Row="6" Foreground="#FFEB0F0F" />
            <TextBlock Height="27" HorizontalAlignment="Left" Margin="-3,0,0,0" Name="textBlock8" Text="TextBlock" VerticalAlignment="Top" Width="739" Grid.Row="7" Foreground="#FFEB0F0F" />
