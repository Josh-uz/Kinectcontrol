        </Grid>
        <CheckBox Canvas.Left="516" Canvas.Top="11" Content="Display VideoFrame" Height="21" Name="checkBox1" Width="231" IsChecked="True" Foreground="#FFEB1818" Click="checkBox1_Click" />
    </Canvas>
</Window>
